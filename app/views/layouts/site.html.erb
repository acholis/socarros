<!DOCTYPE html>
<html>
  <head>
    <title>Socarros</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>

    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">

    <link rel="stylesheet" href="/templete/css/animate.css">
    <link rel="stylesheet" href="/templete/css/owl.carousel.min.css">
    <link rel="stylesheet" href="/templete/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="/templete/css/magnific-popup.css">
    <link rel="stylesheet" href="/templete/css/flaticon.css">
    <link rel="stylesheet" href="/templete/css/style.css">

  </head>

  <body>

    <%= render 'layouts/navbar' %>
    <%= yield %>

    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee"/><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00"/></svg></div>

    <%= render 'layouts/footer' %>

    <script src="/templete/js/jquery.min.js"></script>
    <script src="/templete/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="/templete/js/popper.min.js"></script>
    <script src="/templete/js/bootstrap.min.js"></script>
    <script src="/templete/js/jquery.easing.1.3.js"></script>
    <script src="/templete/js/jquery.waypoints.min.js"></script>
    <script src="/templete/js/jquery.stellar.min.js"></script>
    <script src="/templete/js/owl.carousel.min.js"></script>
    <script src="/templete/js/jquery.magnific-popup.min.js"></script>
    <script src="/templete/js/jquery.animateNumber.min.js"></script>
    <script src="/templete/js/scrollax.min.js"></script>
    <script src="/templete/js/main.js"></script>

  </body>
</html>
